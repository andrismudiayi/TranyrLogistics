@model TranyrLogistics.Models.ShipmentDocument

@{
    ViewBag.Title = "Document Details";
}

<h3>@ViewBag.Title</h3>
<br />

<fieldset>
    <legend>ShipmentDocumentation</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Label)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Label)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Description)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Description)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.CreateDate)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.CreateDate)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.ID }) |
    @Html.ActionLink("Back to List", "Index", new { customer_number=Model.CustomerNumber, shipment_id=Model.ShipmentID })
</p>
